    function QueryPay() {
        var stime = $("#start_datetime").val();
        var etime = $("#end_datetime").val();
        if (stime != "") {
            stime = stime + " 00:00:00";
        }
        if (etime != "") {
            etime = etime + " 23:59:59";
        }
        $("#MoneyPay").hiMallDatagrid({
            url: "/common/PublicMoney/ListPay",
            singleSelect: true,
            pagination: true,
            NoDataMsg: '没有找到您的支付信息.',
            idField: "Id",
            pageSize: 10,
            pageNumber: 1,
            queryParams: { "starttime": stime, "endtime": etime, "moneytype": $('#type_id option:selected').val(), "userid": $("#hid_userid").val(), "usertype": $("#hid_usertype").val() },
            columns:
            [[
                 {
                     field: "PayMent_Number", title: "支付单号", align: "center", formatter: function (value, row, index) {
                         return row.PayMent_Number;
                     }
                 }
                , {
                    field: "PayMent_OrderNum", title: "订单号", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_OrderNum;
                    }
                }
                , {
                    field: "PayMent_MoneyType", title: "支付币种", align: "center", formatter: function (value, row, index) {
                        if (row.PayMent_MoneyType == "1") {
                            return "CNY";
                        } else {
                            return "USD";
                        }
                    }
                }, {
                    field: "PayMent_PayMoney", title: "支付金额", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_PayMoney.toFixed(2);
                    }
