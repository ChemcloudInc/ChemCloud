    <script type="text/javascript">
        $(function () {
            //模糊搜索样式
            var evTimeStamp = 0;
            $('.icheckbox_sm-blue').click(function () {
                var now = +new Date();
                if (now - evTimeStamp < 100) {
                    return;
                }
                evTimeStamp = now;
                if ($('.icheckbox_sm-blue').hasClass('checked')) {
                    $('.icheckbox_sm-blue').removeClass('checked');
                } else {
                    $('.icheckbox_sm-blue').addClass('checked');
                }

            })

            //王传伟，打开 聊天窗口
            $("#btnOpenChatMessageIndex").on("click", function () {

                //window.open("/ChatMessage/Index");

                var w = window.open();
                setTimeout(function () {
                    w.location = "/ChatMessage/Index";
                }, 1000);

                return false;

            });
            //end 王传伟，打开 聊天窗口

            if (location.href.indexOf("c=1") > 0) {
                $("#iframe").attr("src", "/UserInfo/Index");
            } else if (location.href.indexOf("c=2") > 0) {
                $("#iframe").attr("src", "/MessageDetail/Management");
            } else if (location.href.indexOf("c=3") > 0) {
                $("#iframe").attr("src", "/Bargain/BargainList");
            }
            else if (location.href.indexOf("c=5") > 0) {
                $("#iframe").attr("src", "/OrderSynthesis/AddOrderSynthesis");

            }
            else if (location.href.indexOf("c=4") > 0) {
                $("#iframe").attr("src", "/OrderPurchasing/AddOrderPurchasing");
            }
            //下拉菜单
            $('.top .dropdown').hover(function () {
                $(this).toggleClass('hover');
            });

            ////搜索切换
            //$('.search-form label').click(function () {
            //    $(this).siblings('ul').show();
            //});
            //$('.search-form ul li').click(function () {
            //    $(this).parent().hide().siblings('label').text($(this).text());
            //});

            //左侧菜单栏自适应高度
            $('.aside').height($(".aside-list").height() + 800);
            $(window).resize(function () {
                $('.aside').height($(".aside-list").height() + 800);

            });

            //右侧主界面自适应高度
            $('.content').height($(".aside").height() + 800);
            $(window).resize(function () {
                $('.content').height($(".aside").height() + 800);

            });


            $('.aside-list li a').click(function () {
                $(this).parent().addClass('current').siblings().removeClass('current');

                //右侧主界面自适应高度,js获取iframe里页面的高度，给了最外侧的div，iframe高度自适应此div
                if ($("#iframe").contents().find("body").height() > $(".aside-list").height()) {
                    $(".content").height($("#iframe").contents().find("body").height() + 300);

                    $(window).resize(function () {
                        $('.content').height($("#iframe").contents().find("body").height() + 300);
                    });

                } else {
                    $('.content').height($(".aside").height() + 20);

                    $(window).resize(function () {
                        $('.content').height($(".aside").height() + 20);
                    });
                }
            });
            $('#UserAddress_syl').click(function () {
                $(this).parent().addClass('current').siblings().removeClass('current');

                //为收货地址页面写的高度
                $('.content').height($(".aside-list").height() + 2800);
                $(window).resize(function () {
                    $('.content').height($(".aside-list").height() + 2800);
                });
            });
            $('#UserOrder_syl').click(function () {
                $(this).parent().addClass('current').siblings().removeClass('current');

                //为订单管理写的高度
                $('.content').height($(".aside-list").height() + 600);
                $(window).resize(function () {
                    $('.content').height($(".aside-list").height() + 600);
                });
            });

            $('.syl_TechInfo').click(function () {
                $(this).parent().addClass('current').siblings().removeClass('current');

                //为技术资料写的高度
                $('.content').height($(".aside-list").height() + 300);
                $(window).resize(function () {
                    $('.content').height($(".aside-list").height() + 300);
                });
            });
            $('#searchBtn').click(function () {

                var keyWords = $.trim($('#searchBox').val());

                if (keyWords == "") {
                    $.dialog.errorTips("请输入搜索关键字");
                    return false;
                }


                //if (keyWords.length < 3) {
                //    $.dialog.errorTips("搜索长度不能小于3");
                //    return false;
                //}


                if ($("#issearchlike").attr("checked") == "checked") {
                    //模糊搜索
