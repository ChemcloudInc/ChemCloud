            if (myurl != null && myurl.toString().length > 1) {
                var tar = GetQueryString("tar").toLowerCase();
                $(".aside-list li a").each(
                    function (index, item) {
                        if ($(item).attr("href").toLowerCase().indexOf(tar) >= 0) {
                            $(item).parent().click();
                        }
                    }
                );
                window.iframe.location.href = myurl;
            } else {
                $(".nav li").first().click();
            }
