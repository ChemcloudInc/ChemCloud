        function IsExitsOrganization() {
            var userId = $("#currentId").val();
            $.post('/UserCenter/IsAdmin', { userId: userId }, function (result) {
                if (!result.success) {
                    $.dialog.tips("请联系管理员添加角色组和组织架构");
