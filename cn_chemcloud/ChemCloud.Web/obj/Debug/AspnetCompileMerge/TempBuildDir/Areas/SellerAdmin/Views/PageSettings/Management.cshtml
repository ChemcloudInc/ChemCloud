@model IEnumerable<ChemCloud.Model.ImageAdInfo>

<script src="~/Scripts/jquery.ChemCloudUpload.js"></script>
<script type="text/javascript">
    var setlogoUrl = '@Url.Action("setlogo")';
    var logo = "@ViewBag.Logo";
</script>
<script src="~/Areas/SellerAdmin/Scripts/pageSettings.js"></script>
<script type="text/javascript" src="~/Scripts/jquery.nicescroll.min.js"></script>
<div class="container">


    <div class="vis-view-area clearfix">
        <div class="banner-area">
            <a class="logo-area" title="Logo设置"></a>
            <a type="imageAd" imagedescript="1210*90" class="banner-a" title="banner设置" value="@Model.ElementAt(0).Id" pic="@Model.ElementAt(0).ImageUrl" url="@Model.ElementAt(0).Url"></a>
        </div>
        <div class="nav-area">
            <a href="../Navigation/management" title="导航条设置"></a>
        </div>
        <div class="focus-area">
            <a href="SlideAds" title="focus轮播图设置"></a>
        </div>
        <div class="goods-area">
            <a href="../Category/Management" class="shop-goood-sort"></a>
            <div class="recomm-goods-area">
                <a type="imageAd" imagedescript="325*325" class="" title="精品推荐(1)" value="@Model.ElementAt(1).Id" pic="@Model.ElementAt(1).ImageUrl" url="@Model.ElementAt(1).Url"></a>
                <a type="imageAd" imagedescript="325*325" class="" title="精品推荐(2)" value="@Model.ElementAt(2).Id" pic="@Model.ElementAt(2).ImageUrl" url="@Model.ElementAt(2).Url"></a>
                <a type="imageAd" imagedescript="325*325" class="recomm-area" title="精品推荐(3)" value="@Model.ElementAt(3).Id" pic="@Model.ElementAt(3).ImageUrl" url="@Model.ElementAt(3).Url"></a>
            </div>
            <div class="floors-area">
                <a href="../ShopHomeModule/management" title="楼层编辑"></a>
            </div>
        </div>


    </div>


</div>
<div class="dialog-form" id="logosetting" style="display:none">
    <div class="form-group upload-img clearfix" id="uploadImg">
    </div>
</div>
