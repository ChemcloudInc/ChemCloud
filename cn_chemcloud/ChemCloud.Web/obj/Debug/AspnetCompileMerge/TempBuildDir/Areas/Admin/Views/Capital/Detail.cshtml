
@{
    
}
<script src="~/Scripts/jquery.ChemCloudDatagrid.js"></script>
<script src="~/Scripts/jquery.ChemCloudLinkage.nocreate.js"></script>
<script src="~/Areas/Admin/Scripts/CapitalDetail.js"></script>
<link rel="stylesheet" type="text/css" href="/Content/bootstrap-datetimepicker.min.css">
<script type="text/javascript" src="/Scripts/bootstrap-datetimepicker.js"></script>
<script type="text/javascript" src="/Scripts/bootstrap-datetimepicker.zh-CN.js"></script>
<div class="container">
    <ul class="nav nav-tabs-custom clearfix">
        <li class ="active"><a href="../Index">预付款管理</a></li >
        <li ><a href="../WithDraw">提现管理</a></li>
    </ul>

    <div class="search-box" id="search-box">

         <form class="custom-inline" role="form">
          <ul class="cl two-title" id="ulstatus">
                 <li value="0" class=active" ><a >账户明细</a></li>

                 @foreach (var d in ChemCloud.Core.EnumHelper.ToDescriptionDictionary<ChemCloud.Model.CapitalDetailInfo.CapitalDetailType>())
                 {
                     <li value="@d.Key"><a >@d.Value</a></li>
                 }
             </ul>
            <label class="label-inline" for="">时间</label>
            <div class="form-group">
                <input id="inputStartDate" class="form-control input-ssm start_datetime" type="text">
            </div>
            <label> -- </label>
            <div class="form-group">
                <input id="inputEndDate" class="form-control input-ssm end_datetime" type="text">
            </div>
            <span class="submit"><button type="button" class="btn btn-primary btn-sm" id="searchButton" onclick="search()">搜索</button></span>
        </form>
    </div>

    <table id="list" class="table"></table>
    <input type="hidden" id="userid" value="@ViewBag.UserId"/>
</div>