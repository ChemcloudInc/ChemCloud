@model IEnumerable<ChemCloud.Model.CustomerServiceInfo>
@foreach (var m in Model)
{
    if (m.Tool == ChemCloud.Model.CustomerServiceInfo.ServiceTool.QQ)
    {
        <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=@m.AccountCode&amp;site=qq&amp;menu=yes" title="@m.Name">
            <img border="0" src="http://wpa.qq.com/pa?p=2:@m.AccountCode:52" style="vertical-align: middle;" />
        </a>
    }
    else if (m.Tool == ChemCloud.Model.CustomerServiceInfo.ServiceTool.Wangwang)
    {
            <!-- wang wang -->
        <a target="_blank" href="http://amos.alicdn.com/msg.aw?v=2&uid=@m.AccountCode&site=cnalichn&s=11&charset=UTF-8" class="vm" title="@m.Name">
            <img border="0"
                 src="http://amos.im.alisoft.com/online.aw?v=2&amp;uid=@m.AccountCode&amp;site=cntaobao&amp;s=2&amp;charset=UTF-8" alt="Wang Wang"
                 style=" vertical-align: middle;">
        </a>
    }
}
