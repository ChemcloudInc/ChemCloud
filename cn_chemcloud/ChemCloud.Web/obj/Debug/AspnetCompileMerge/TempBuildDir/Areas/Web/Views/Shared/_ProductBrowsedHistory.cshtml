@model  IEnumerable<ChemCloud.Web.Areas.Web.Models.ProductBrowsedHistoryModel>
<div class="story_box">

    @foreach(var m in Model)
    {
        if (m == null) { continue; }
     <a target="_blank" href="@Url.Action("Detail", "Product", new { id = @m.ProductId })" class="story_p">
        <div class="c"><img width="100" height="100" style="mrb10" src="@m.ImagePath/1_100.png"/></div>
        <span class="linebr story_des">@m.ProductName</span>
        <span class="story_price"><b class="desc">价格: </b>￥@m.ProductPrice.ToString("f2")</span>
    </a>
    }
</div>
