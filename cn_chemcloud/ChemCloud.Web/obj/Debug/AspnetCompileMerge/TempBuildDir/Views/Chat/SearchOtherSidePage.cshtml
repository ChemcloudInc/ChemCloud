
@model IEnumerable<ChemCloud.Web.Models.OtherSideViewModel>
@{
    Layout = null;
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>名称</th>
            <th>操作</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
             
                <tr  data-user-id="@item.UserID" data-user-name="@item.UserName">
                    <td >@item.UserName</td>
                    <td>
                        <div class="btn-group">
                            <button type="button" class="btn btn-success">收藏</button>
                        </div>
                     </td>
                 </tr>
 
        }

    </tbody>
</table>



