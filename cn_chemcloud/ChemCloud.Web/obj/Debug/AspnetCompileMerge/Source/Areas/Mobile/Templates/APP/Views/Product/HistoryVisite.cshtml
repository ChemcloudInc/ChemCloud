@model List<ChemCloud.Web.Areas.Web.Models.ProductBrowsedHistoryModel>
@{
    ViewBag.Title = "我的足迹";
}
<div class="container">
    
    @foreach (var product in Model)
    { 
        <h3 class="history-hd">@product.BrowseTime</h3>
        <ul class="history-list clearfix">
            <li>
                <a class="p-img" href="@Url.Action("Detail", new { id = @product.ProductId })"><img class="lazyload" src="@(product.ImagePath)/1_150.png" width="100%" data-url="@(product.ImagePath)/1_150.png" alt=""></a>
                <h3><a>@product.ProductName</a></h3>
                <p>¥ @product.ProductPrice</p>
            </li>
        </ul>
    }
    <div class="mt20"></div>
</div>