                </tr>
            }

        </tbody>
    </table>*@

    <h3 class="table-hd">平台审核信息</h3>
    <table class="table table-bordered table-striped">
        <colgroup>
            <col class="col-xs-1">
            <col class="col-xs-7">
        </colgroup>
        <tbody>
            <tr>
                <td>审核状态</td>
                <td>@ViewBag.PassStr</td>
            </tr>
            @if ((int?)ViewBag.Status == 4)
            {
                <tr>
                    <td>未通过原因</td>
                    <td>@Model.RefuseReason</td>
                </tr>
            }
        </tbody>
    </table>
</div>
