    function QueryWithDraw() {
        $("#MoneyWithDraw").hiMallDatagrid({
            url: "/common/PublicMoney/ListWithDraw",
            singleSelect: true,
            pagination: true,
            NoDataMsg: '没有找到您的提现信息.',
            idField: "Id",
            pageSize: 10,
            pageNumber: 1,
            queryParams: { "starttime": $("#start_datetime2").val(), "endtime": $("#end_datetime2").val(), "moneytype": $('#type_id2 option:selected').val(), "userid": $("#hid_userid").val(), "usertype": $("#hid_usertype").val() },
            columns:
            [[
                 {
                     field: "Withdraw_Number", title: "提现单号", width:150,align: "center", formatter: function (value, row, index) {
                         return row.Withdraw_Number;
                     }
                 }
                , {
                    field: "Withdraw_MoneyType", title: "提现币种", width:50, align: "center", formatter: function (value, row, index) {
                        if (row.Withdraw_MoneyType == "1") {
                            return "CNY";
                        } else {
                            return "USD";
                        }
                    }
                }, {
                    field: "Withdraw_Money", title: "提现金额", width:70,align: "center", formatter: function (value, row, index) {
                        return row.Withdraw_Money.toFixed(2);
                    }
                }, {
                    field: "Withdraw_BankName", title: "提现银行", width:100,align: "center", formatter: function (value, row, index) {
                        return row.Withdraw_BankName;
                    }
                }, {
                    field: "Withdraw_BankUserName", title: "提现人名", width:70,align: "center", formatter: function (value, row, index) {
                        return row.Withdraw_BankUserName;
                    }
                }, {
                    field: "Withdraw_Account", title: "提现账户", width:150,align: "center", formatter: function (value, row, index) {
                        return row.Withdraw_Account;
                    }
                }, {
                    field: "Withdraw_Time", title: "提现时间", width:100,align: "center", formatter: function (value, row, index) {
                        var time = row.Withdraw_Time;
                        return jsonDateFormat(time);
                    }
                }, {
                    field: "Withdraw_shenhe", title: "处理状态", width:100,align: "center", formatter: function (value, row, index) {
                        var sstau = row.Withdraw_shenhe;
                        if (sstau == "0") {
                            return "<span style=\"color:green;\">提现处理中</span>";
                        } else if (sstau == "1") {
                            return "<span style=\"color:blue;\">提现完成</span>";
                        } else {
                            return "<span style=\"color:red;\">提现失败</span>";
                        }
                    }
                }
            ]]
        });
    };

