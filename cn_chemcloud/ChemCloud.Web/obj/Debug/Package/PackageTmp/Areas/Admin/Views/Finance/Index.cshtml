<script type="text/javascript">
    $(function () {
        $("#tabs").tabs();
        QueryPay();
        QueryInCome();
        QueryWithDraw();
        QueryRefund();
        QueryTrans();
        QueryRecharge();
        $("#searchBtn").on("click", function () {
            QueryPay();
        });
        $("#searchBtn1").on("click", function () {
            QueryInCome();
        });
        $("#searchBtn2").on("click", function () {
            QueryWithDraw();
        });
        $("#searchBtn3").on("click", function () {
            QueryRecharge();
        });
        $("#searchBtn4").on("click", function () {
            QueryRefund();
        });
        $("#searchBtn5").on("click", function () {
            QueryTrans();
        });
    });

    function QueryPay() {

        $("#MoneyPay").hiMallDatagrid({
            url: "/common/PublicMoney/ListPay",
            singleSelect: true,
            pagination: true,
            NoDataMsg: '没有找到您的支付信息.',
            idField: "Id",
            pageSize: 10,
            pageNumber: 1,
            queryParams: { "starttime": $("#start_datetime").val(), "endtime": $("#end_datetime").val(), "moneytype": $('#type_id option:selected').val(), "userid": $("#hid_userid").val(), "usertype": $("#hid_usertype").val() },
            columns:
            [[
                 {
                     field: "PayMent_Number", title: "支付单号", align: "center", formatter: function (value, row, index) {
                         return row.PayMent_Number;
                     }
                 }
                , {
                    field: "PayMent_OrderNum", title: "订单号", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_OrderNum;
                    }
                }
                , {
                    field: "PayMent_MoneyType", title: "支付币种", align: "center", formatter: function (value, row, index) {
                        if (row.PayMent_MoneyType == "1") {
                            return "CNY";
                        } else {
                            return "USD";
                        }
                    }
                }, {
                    field: "PayMent_PayMoney", title: "支付金额", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_PayMoney.toFixed(2);
                    }
                }, {
                    field: "PayMent_SXMoney", title: "手续费", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_SXMoney.toFixed(2);
                    }
                }, {
                    field: "PayMent_YFMoney", title: "运费", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_YFMoney.toFixed(2);
                    }
                }, {
                    field: "PayMent_BXMoney", title: "保险费", align: "center", formatter: function (value, row, index) {
                        var bxmoney = row.PayMent_BXMoney;
                        return bxmoney.toFixed(2);
                    }
                }, {
                    field: "PayMent_JYMoney", title: "交易费", align: "center", formatter: function (value, row, index) {
                        return row.PayMent_JYMoney.toFixed(2);
                    }
                }, {
                    field: "PayMent_PayTime", title: "支付时间", align: "center", formatter: function (value, row, index) {
                        var time = row.PayMent_PayTime;
                        return jsonDateFormat(time);
                    }
                }
                //, {
                //    field: "PayMent_PayAddress", title: "支付地点", align: "center", formatter: function (value, row, index) {
                //        return row.PayMent_PayAddress;
                //    }
                //}
            ]]
        });
    };

