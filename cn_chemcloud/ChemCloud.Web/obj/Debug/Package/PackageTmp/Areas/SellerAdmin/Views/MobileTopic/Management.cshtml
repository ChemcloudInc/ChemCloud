<div class="container">
    <ul class="nav nav-tabs-custom clearfix">
        <strong>移动专题</strong>
        <li class="active"><a href="Management">专题列表</a></li>
        <li><a href="Save">新增</a></li>
    </ul>
    <div class="search-box only-line">
        <form class="custom-inline">
            <div class="form-group">
                <label class="label-inline" for="">标题</label>
                <input class="form-control input-ssm" type="text" id="titleKeyword">
            </div>
            <div class="form-group">
                <label class="label-inline" for="">标签</label>
                <input class="form-control input-ssm" type="text" id="tagsKeyword">
            </div>
            <button type="button" class="btn btn-primary btn-sm" id="searchButton">搜 索</button>
        </form>
    </div>
    <table id="topicGrid" class="table mt20"></table>
</div>

<div class="mobile-dialog">
    <iframe name="mobile-iframe" id="mobileshow" frameborder="0" scrolling="auto" src="about:blank"></iframe>
</div>
<div class="cover"></div>
<script type="text/javascript">
    $( document ).on( 'click', '.view-mobile-shop', function ()
    {
        $( "#mobileshow" ).attr( "src", $( this ).attr( "data-url" ) );
        $( '.mobile-dialog' ).show();
        $( '.cover' ).fadeIn();
    } );
    $( '.cover' ).click( function ()
    {
        $( '.mobile-dialog' ).hide();
        $( "#mobileshow" ).attr( "src", "about:blank" );
        $( '.cover' ).fadeOut();
    } );
</script>
<script src="~/Scripts/jquery.zclip.js"></script>
<script src="~/Scripts/jquery.ChemCloudDatagrid.js"></script>
<script src="~/Areas/SellerAdmin/Scripts/mobileTopicManagement.js"></script>
