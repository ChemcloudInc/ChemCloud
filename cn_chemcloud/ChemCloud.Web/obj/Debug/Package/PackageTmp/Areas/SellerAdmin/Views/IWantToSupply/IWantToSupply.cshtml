<div class="div_web_content" style="padding-top:20px;width:1210px">
    <div class="title">
        <span class="title_txt cur">竞价列表</span>
    </div>

    <div class="search-area">
        <label>产品名称：</label>
        <input class="search_order" id="txtkeyword" type="text" placeholder="请输入职位名称" />
        <label>发布日期：</label>
        <input id="inputStartDate1" class="start_datetime" type="text">到<input id="inputEndDate1" class="start_datetime" type="text">
        <label>交付日期：</label>
        <input id="inputStartDate2" class=" start_datetime" type="text">到<input id="inputEndDate2" class="start_datetime" type="text">
        @*<select id="selectAllOrMine" class="search_dll">
            <option value="0">我参与的</option>
            <option value="1">全部</option>
        </select>*@
        <button id="btn_Search" class="btn_SearchAddDel">搜索</button>
    </div>
    <div class="div_list">
        <table class="tb-list">
            <colgroup>
                <col width="5%" />
                <col width="15%" />
                <col width="5%" />
                <col width="5%" />
                <col width="5%" />
                <col width="5%" />
                @*<col width="12%" />*@
                <col width="15%" />
                <col width="5%" />
                <col width="15%" />
                <col width="25%" />
            </colgroup>
            <thead>
                <tr class="tr" style="background-color:#e6e6e6;">
                    <th style="background-color:#e6e6e6;">序号</th>
                    <th style="background-color:#e6e6e6;">产品名称</th>
                    <th style="background-color:#e6e6e6;">采购单价</th>
                    <th style="background-color:#e6e6e6;">采购总价</th>
