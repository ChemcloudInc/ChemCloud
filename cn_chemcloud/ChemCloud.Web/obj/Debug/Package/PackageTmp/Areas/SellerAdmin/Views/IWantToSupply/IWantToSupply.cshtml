    .search-area label { width: 70px; }

.span_wytb_lan { padding: 1px 10px; font-size: 12px; line-height:30px; cursor: pointer; border-radius: 3px; text-align: center; color: #fff; background-color: #2289ce; }
.span_wytb_lan1 { padding: 0px 10px; font-size: 12px;line-height:30px; cursor: pointer; border-radius: 3px; text-align: center; color: #fff; background-color: rgb(66, 133, 244); }
