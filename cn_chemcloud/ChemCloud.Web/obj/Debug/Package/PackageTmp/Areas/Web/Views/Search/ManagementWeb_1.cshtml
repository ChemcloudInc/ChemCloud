@{
    //Layout = "~/Areas/Web/Views/Shared/_UserCenter.cshtml";
    ViewBag.Title = "ChemCloud-知识产权";
}
<link href="~/Areas/Web/Content/SearchListPager.css" rel="stylesheet" />
<link href="~/Areas/Web/Content/user-center.css" rel="stylesheet" />
<!--时间空间-->
<link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/bootstrap-datetimepicker.zh-CN.js"></script>
<script src="~/Scripts/datetimeCustom.js"></script>
<!--本页-->
<script src="~/Scripts/jquery.ChemCloudUpload.js"></script>
<script src="~/Scripts/jquery.ChemCloudLinkage.nocreate.js"></script>
<script src="~/Areas/Admin/Scripts/upload/browser.js"></script>
<script src="~/Areas/Admin/Scripts/upload/jquery.form.js"></script>

<!--分页-->
<link href="~/Areas/Web/Content/JobWeb.css" rel="stylesheet" />
<script src="~/Areas/Web/Scripts/js_web/left_nav.js"></script>
<script type="text/javascript">
    $(function () {
        var rkey = '@Request.QueryString["key"]';
        if (rkey != "" && rkey != null) {
            $("#hidsearchtxt").val(rkey);
        }
    });
</script>
<script src="~/Areas/Web/Scripts/js_web/ManagementLawInfoWeb.js" type="text/javascript"></script>
<script src="~/Areas/Web/Scripts/js_web/pageGroup.js"></script>
<style>
    .falu {
        margin: 0 auto;
    }

        .falu li {
            border-bottom: 1px dashed #ccc;
            height: 96px;
        }

        .falu h3 {
            font-size: 14PX;
            height: 24px;
            margin-top: 10px;
        }

        .falu .syl_img_text {
            float: right;
            text-align: left;
            line-height: 24px;
            width: 1050px;
            margin-top: 10px;
        }

        .falu .syl_text {
            text-align: left;
            line-height: 24px;
            width: 1050px;
            margin-top: 10px;
        }

        .falu h3 a {
            color: #3480e8;
            font-size: 16px;
        }

        .falu img {
            width: 74px;
            height: 40px;
            margin-top: 14px;
        }

        .falu h3 img {
            width: 6px;
            height: 8px;
            margin-bottom: 1px;
            margin-right: 10px;
        }

    .div_search .input_Search {
        width: 400px;
        height: 30px;
        border: 2px solid #000;
        background: #fff;
        border-radius: 4px;
    }

    .div_search #btn_Search {
        border: 2px solid #000;
        background: #fff;
        color: #e4393c;
        height: 34px;
        border-radius: 4px;
    }

    .background_search {
        width: 20px;
        height: 20px;
        display: inline-block;
        margin-left: -26px;
        margin-bottom: -5px;
        z-index: 2;
        background: url(/Areas/Web/Images/zoom49.png) no-repeat;
        background-size: 20px;
    }
</style>
<div class="main">
    <div class="div_search1">
        <div class="div_search">
            <label>内容：</label>
            <input id="input_keyword" type="text" />
            <label>标题：</label>
            <input id="input_content" type="text" />
            <label>起始时间：</label>
            <input id="startDate" type="text" class="dateInput" />
            <label>结束时间：</label>
            <input id="endDate" type="text" value="" class="dateInput" />
            <button id="btn_Search" style="cursor:pointer">搜索</button>
        </div>
    </div>
    <div class="div_jobs_list">
        <div class="falu">
            <ul id="ul_imglist"></ul>
        </div>
        @*<ul id="ul_jobs_list"></ul>*@
        <!-------------------------------------------分页----------------------------------------------------------------->
        <div id="pageGro" class="cb" style="float: left;margin-top: 10px;">
            <div id="preSpanBtn">上一页</div>
            <div class="pageList">
                <ul>
                    <li>1</li>
                    <li>2</li>
                    <li>3</li>
                    <li>4</li>
                    <li>5</li>
                </ul>
            </div>
            <div id="nextSpanBtn">下一页</div>
        </div>
        <!-------------------------------------------END 分页----------------------------------------------------------------->

    </div>

</div>
<input type="hidden" id="hidsearchtxt" value="" />


